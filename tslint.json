{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "double"],
        "ter-indent": [true, 4],
        "object-literal-sort-keys": [false],
        "ordered-imports": [true, {
            "grouped-imports": true
        }]
    },
    "rulesDirectory": []
}